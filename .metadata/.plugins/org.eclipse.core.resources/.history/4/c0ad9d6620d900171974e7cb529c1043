import java.util.Scanner;

public class SpaceBoxing {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		
		double weight;
		
		System.out.print( "Please enter your current weight on Earth: " );
		weight = input.nextDouble();
		
		System.out.println( "Choose from one of these planets: ");
		System.out.printf( "%1$s-15 %2$s-15 %3$s-15 \n%4$s-15 %5$s-15 %6$s-15", "1. Venus", "2. Mars", "3. Jupiter", "4. Saturn", "5. Uranus", "6. Neptune" );
		
	}
	
}
