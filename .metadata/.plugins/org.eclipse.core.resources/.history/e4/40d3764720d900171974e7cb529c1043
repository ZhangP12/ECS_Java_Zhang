import java.util.Scanner;

public class SpaceBoxing {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		
		double weight;
		
		System.out.print( "Please enter your current weight on Earth." );
		weight = input.nextDouble();
		
		System.out.println( "Choose from one of these planets: ");
		System.out.printf( "%1$s- %2$s- %3$s- \n%4$s- %5$s- %6$s-", "1. Venus", "2. Mars", "3. Jupiter", "4. Saturn", "5. Uranus", "6. Neptune" );
		
	}
	
}
